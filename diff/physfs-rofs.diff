+static int rofs_read_filename(void *fh, unsigned char *dst, int dst_len)
+{
+    int i = 0;
+
+    memset(dst, '\0', dst_len);
+
+    do {
+        unsigned char c;
+
+        if (__PHYSFS_platformRead(fh, &c, 1, 1) != 1) {
+            return(0);
+        }
+        dst[i] = tolower(c);
+    } while (dst[i++] != '\0');
+
+    return(1);
+}
